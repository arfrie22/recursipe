{
  "name": "recursipe",
  "version": "1.0.0",
  "description": "",
  "main": "src/server/index.ts",
  "type": "module",
  "scripts": {
    "build": "parcel build",
    "start": "PUBLIC_DIR=\"dist/public\" node dist/index.js",
    "launch": "(npm run build && exec npm run start)",
    "watch": "nodemon --watch src --watch . --exec 'npm run launch' --ext ts,html,ejs,css,env.*,env",
    "serve": "parcel serve --target frontend"
  },
  "keywords": [],
  "author": {
    "name": "Andrew Friedman",
    "email": "arfriedman@umass.edu"
  },
  "license": "ISC",
  "dependencies": {
    "@auth/express": "^0.5.5",
    "@auth/typeorm-adapter": "^2.2.0",
    "@parcel/macros": "^2.12.0",
    "@shopify/draggable": "^1.1.3",
    "@types/pg": "^8.11.6",
    "dotenv": "^16.4.5",
    "edge.js": "^6.0.2",
    "express": "^4.19.2",
    "lucide": "^0.395.0",
    "next-auth": "5.0.0-beta.19",
    "pg": "^8.12.0",
    "rollup": "^4.18.0",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.5",
    "uuid": "^10.0.0"
  },
  "@parcel/transformer-js": {
    "inlineFS": false
  },
  "targets": {
    "frontend": {
      "source": "src/public/index.html",
      "distDir": "dist/public",
      "context": "browser"
    },
    "backend": {
      "source": "src/server/index.ts",
      "distDir": "dist",
      "context": "node",
      "outputFormat": "esmodule"
    }
  },
  "devDependencies": {
    "@parcel/config-default": "^2.12.0",
    "@parcel/packager-raw-url": "2.12.0",
    "@parcel/transformer-raw": "^2.12.0",
    "@parcel/transformer-webmanifest": "2.12.0",
    "@rollup/plugin-swc": "^0.3.1",
    "@rollup/plugin-typescript": "^11.1.6",
    "@swc/core": "^1.6.5",
    "@types/express": "^4.17.21",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^5.5.0||^6.0.0",
    "crypto-browserify": "^3.12.0",
    "daisyui": "^4.12.2",
    "events": "^3.1.0",
    "nodemon": "^3.1.3",
    "parcel": "^2.12.0",
    "parcel-transformer-ejs": "^1.0.1",
    "path-browserify": "^1.0.0",
    "postcss": "^8.4.38",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.1.0",
    "string_decoder": "^1.3.0",
    "tailwindcss": "^3.4.4",
    "url": "^0.11.0",
    "util": "^0.12.3",
    "vm-browserify": "^1.1.2"
  }
}
